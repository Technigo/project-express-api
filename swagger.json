{
    "swagger": "2.0",
    "info": {
      "title": "Top 50 Tech Companies API",
      "version": "1.0.0"
    },
    "host": "project-express-api-cvzekbgn3q-lz.a.run.app",
    "basePath": "/",
    "schemes": [
      "https"
    ],
    "paths": {
        "/": {
            "get": {
              "description": "Index route - displays a welcome message",
              "produces": [
                "text/plain"
              ],
              "responses": {
                "200": {
                  "description": "Welcome message displayed successfully"
                }
              }
            }
          },
      "/companies": {
        "get": {
          "description": "Returns the full data for all companies",
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "Data returned successfully"
            }
          }
        }
      },
      "/companies/{name}": {
        "get": {
          "description": "Returns data for companies by name",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "name",
              "in": "path",
              "description": "Company name",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "Data returned successfully"
            },
            "404": {
              "description": "Company not found"
            }
          }
        }
      },
      "/companies/states/{state}": {
        "get": {
          "description": "Returns data for all companies with a HQ in the specified state",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "state",
              "in": "path",
              "description": "State where the company's HQ is located",
              "required": true,
              "type": "string",
              "enum": [
                "arizona",
                "california",
                "connecticut",
                "florida",
                "idaho",
                "massachusetts",
                "montana",
                "new jersey",
                "new york",
                "north carolina",
                "texas",
                "washington",
                "wisconsin"
              ]
            }
          ],
          "responses": {
            "200": {
              "description": "Data returned successfully"
            },
            "404": {
              "description": "No companies found with HQ in the specified state"
            }
          }
        }
      },
      "/companies/sectors": {
        "get": {
          "description": "Returns data for companies in the specified sector",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "sectors",
              "in": "query",
              "description": "Sector the company operates in",
              "required": true,
              "type": "string",
              "enum": [
                "communication equipments",
                "computer hardware",
                "consumer electronics",
                "electronic components",
                "it services",
                "semiconductors",
                "software application",
                "software infrastructure"
              ]
            }
          ],
          "responses": {
            "200": {
              "description": "Data returned successfully"
            },
            "404": {
              "description": "No companies found in the specified sector"
            }
          }
        }
      },

      "/companies/years/{year}": {
        "get": {
          "description": "Returns data for all companies founded in the specified year",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "year",
              "in": "path",
              "description": "Year the company was founded",
              "required": true,
              "type": "integer"
            }
          ],
          "responses": {
            "200": {
              "description": "Data returned successfully"
            },
            "404": {
              "description": "No companies found founded in the specified year"
            }
          }
        }
      },
      "/companies/sectors/{sector}/{state}": {
        "get": {
          "description": "Returns data for companies in the specified sector with a HQ in the specified state",
          "produces": [
            "application/json"
          ],
          "parameters": [
            {
              "name": "sector",
              "in": "path",
              "description": "Sector the company operates in",
              "required": true,
              "type": "string",
              "enum": [
                "communication equipments",
                "computer hardware",
                "consumer electronics",
                "electronic components",
                "it services",
                "semiconductors",
                "software application",
                "software infrastructure"
              ]
            },
            {
              "name": "state",
              "in": "path",
              "description": "State where the company's HQ is located",
              "required": true,
              "type": "string",
              "enum": [
                "arizona",
                "california",
                "connecticut",
                "florida",
                "idaho",
                "massachusetts",
                "montana",
                "new jersey",
                "new york",
                "north carolina",
                "texas",
                "washington",
                "wisconsin"
              ]
            }
          ],
          "responses": {
            "200": {
              "description": "Data returned successfully"
            },
            "404": {
              "description": "No companies found matching the specified sector and state criteria"
            }
          }
        }
      },
      "/*": {
        "get": {
          "description": "Error route - displays an error message for unrecognized routes",
          "produces": [
            "text/plain"
          ],
          "responses": {
            "200": {
              "description": "Error message displayed successfully"
            }
          }
        }
      }
    }
  }